{
  "name": "vuecli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "sass": "^1.69.0",
    "vue": "^2.6.14",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuelidate": "^0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^9.0.0",
    "sass-loader": "^13.3.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0,
      "vue/no-mutating-props": 0
    },
    "overrides": [
      {
        "rules": {
          "vue/multi-word-component-names": 0
        },
        "files": [
          "src/views/**/*.vue"
        ]
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
